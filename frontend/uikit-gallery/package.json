{
  "name": "@liquity2/uikit-gallery",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "pnpm build-uikit && pnpm panda-codegen && pnpm cosmos-export && next build",
    "build-static": "pnpm build && cp -r ./out/_next ./cosmos-export && cp -r ./out/cosmos ./cosmos-export",
    "build-uikit": "cd ../uikit && pnpm build",
    "cosmos": "cosmos --expose-imports",
    "cosmos-export": "cosmos-export --expose-imports",
    "dev": "next dev --port 3001",
    "lint": "next lint",
    "panda-codegen": "panda codegen --silent",
    "start": "next start"
  },
  "dependencies": {
    "@liquity2/uikit": "workspace:*",
    "@react-spring/web": "^9.7.3",
    "dnum": "^2.11.0",
    "geist": "^1.3.0",
    "next": "14.0.4",
    "react": "^18",
    "react-cosmos-core": "^6.1.1",
    "react-cosmos-plugin-boolean-input": "^6.1.1",
    "react-dom": "^18",
    "ts-pattern": "^5.1.1"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.37.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.2",
    "react-cosmos": "^6.1.1",
    "react-cosmos-next": "^6.1.1",
    "typescript": "^5.4.5"
  }
}
