{
  "name": "@liquity2/uikit-gallery",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "pnpm panda-codegen && next build",
    "cosmos": "cosmos --expose-imports",
    "dev": "next dev --port 3001",
    "lint": "next lint",
    "panda-codegen": "panda codegen --silent",
    "start": "next start",
    "static-build": "cosmos-export --expose-imports && pnpm build && cp -R ./out/_next ./cosmos-export && cp -R ./out/uikit ./cosmos-export"
  },
  "dependencies": {
    "@liquity2/uikit": "workspace:*",
    "@react-spring/web": "^9.7.3",
    "next": "14.2.2",
    "react": "^18",
    "react-cosmos-plugin-boolean-input": "^6.1.1",
    "react-dom": "^18",
    "ts-pattern": "^5.0.8"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.37.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.2",
    "react-cosmos": "^6.1.1",
    "react-cosmos-next": "^6.1.1",
    "typescript": "^5.4.5"
  }
}
